{
  "name": "workspace-code-snippets",
  "displayName": "Workspace Code Snippets",
  "author": "Felix Swinkels",
  "publisher": "feli0x",
  "homepage": "https://felix.surf/",
  "description": "Create code snippets in your workspace and share them with your team.",
  "repository": "https://github.com/feli0x/workspace-code-snippets",
  "version": "0.3.0",
  "icon": "assets/icon.png",
  "engines": {
    "vscode": "^1.83.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.workspaceCodeSnippet",
        "title": "New Workspace Code Snippet",
        "category": "Snippets"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "extension.workspaceCodeSnippet",
          "group": "navigation"
        }
      ]
    },
    "keybindings": [
      {
        "command": "extension.workspaceCodeSnippet",
        "key": "cmd+k cmd+z"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Workspace Code Snippets",
      "properties": {
        "prefixSymbol": {
          "type": "string",
          "default": "/"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.82.0",
    "@types/mocha": "^10.0.2",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "eslint": "^8.50.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "typescript": "^5.2.2",
    "ts-loader": "^9.4.4",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "@vscode/test-electron": "^2.3.4"
  }
}
